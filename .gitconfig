[user]
	name = Itaraju Brum
	email = ib246@cornell.edu
[core]
[diff]
	tool = meld
[difftool]
	prompt = false
[merge]
	tool = meld
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[alias]
	tree = log --oneline --graph --all
	l = log --pretty=format:'%C(auto)%h %C(Cyan)%ad%C(auto) %d%s' --date=short
	wdiff = diff --color-words='[^[:space:]]|([[:alnum:]]|UTF_8_GUARD)+'
        s = status -s
